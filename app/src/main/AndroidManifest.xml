<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sasankasandes.sparktec">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AttendanceFingerprintActivity">
            <intent-filter>
                <action android:name="com.example.sasankasandes.sparktec.AttendanceFingerprintActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".OBDpairActivity">
            <intent-filter>
                <action android:name="com.example.sasankasandes.sparktec.OBDpairActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".AttendanceFeedback">
            <action android:name="com.example.sasankasandes.sparktec.AttendanceFeedback" />

            <category android:name="android.intent.category.DEFAULT" />
        </activity>
    </application>

</manifest>